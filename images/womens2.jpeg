dataInjection.storeSitePerfExps(val);
}
/*Speedtrap*/
window.QUBITEXP2 = (typeof QUBITEXP2 === 'undefined') ? val : QUBITEXP2+ '|'+ val;
/*GA*/
shopdirect.util.eventGoogle('Experiments', 'Fire', val, true);
};
