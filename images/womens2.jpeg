(function($){
var dataInjection = {};
dataInjection.reformatData = function() {
/* gets the data from speedTrapExperiments reformats it and sends to all endpoints*/
var expId = $('#speedTrapExperiments'),
val, valArr, valCleaned, expVal, i, adrumVar;
if (expId.length && expId.val() !== '{}') {
val = expId.val();
if (val.indexOf(',') !== -1) {
valArr = val.split(',');
for (i = 0; i < valArr.length; i++) {
valCleaned = valArr[i].split('=')[1].replace(/[^A-Za-z0-9\-]/g, '');
dataInjection.inject(valCleaned);
if (i === 0) {
expVal = valCleaned;
} else {
expVal = expVal + '|' + valCleaned;
} 
}
} else {
expVal = val.split('=')[1].replace(/[^A-Za-z0-9\-]/g, '');
dataInjection.inject(expVal);
} 
}
};
dataInjection.reinjectSitePerfExps = function() {
/*SitePerf reinject*/
if (shopdirect.util.cookie.get('spExps')) {
cookieVal = JSON.parse(shopdirect.util.cookie.get('spExps'));
for (var i in cookieVal) {
if (cookieVal[i] === true) {
if (typeof QUBITEXP2 === 'undefined') {
window.QUBITEXP2 = i; 
} else {
if (QUBITEXP2.indexOf(i) === -1) {
window.QUBITEXP2 = QUBITEXP2+ '|'+ i;
}
} 
} 
cookieVal[i] = false;
}
shopdirect.util.cookie.set('spExps', JSON.stringify(cookieVal), 0.05);
} 
};
dataInjection.storeSitePerfExps = function(val) {
var cookieVal;
/*Creates a cookie to store the information about site performance exps*/
if (shopdirect.util.cookie.get('spExps')) {
cookieVal = JSON.parse(shopdirect.util.cookie.get('spExps')); 
} else {
cookieVal = {} 
}
cookieVal[val] = true;
shopdirect.util.cookie.set('spExps', JSON.stringify(cookieVal), 0.05); 
};
dataInjection.inject = function(val) {
/*responsible for sending the data to endpoints*/
var type = val.indexOf('SP-') !== -1 ? 'sitePerfExp' : 'sstExp'; 
