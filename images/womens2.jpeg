<script type="text/javascript">
(function ($, speedtrapId) {
'use strict';
window.SOASTA = window.SOASTA || {};
SOASTA.pg = '';
function speedtrapCheck(time) {
/*
Polls for variable called SDCSAPageID,
Defensive coding for scenarios where the variable is below this script
*/
var pageId = window.SDCSAPageID || null;
if (pageId !== null) {
stringSearch(pageId);
} else {
if (time < 10000) {
time = time * 2;
setTimeout(function () {
speedtrapCheck(time);
}, time);
}
} 
};
function stringSearch(data) { 
/* Split the variable to identify various pages */ 
var pageIdValue = data.split(' | ')[2].toUpperCase();
switch(pageIdValue) {
case 'GALLERY':
galleryPageData(data);
break;
case 'ZONES':
case 'CHECKOUT':
case 'MY ACCOUNT':
case 'FINANCIAL SERVICES': 
pageGroupsData(pageIdValue, data);
break;
default:
pageIdValue = pageIdValue.replace(' |', '');
/*On some pages the value is empty*/
if (pageIdValue !== 'NULL') {
sendData(pageIdValue);
} else {
getPageNameFromUrl();
}
}
};
function getPageNameFromUrl() { 
/* Split the variable to identify various pages */ 
var url = window.location.pathname.split('.')[0].replace('/','').toUpperCase();
sendData(url); 
};
function galleryPageData(data) {
/* Differentiate between normal, search, 12 and 99 pro1ducts */
var valForSoasta, 
numProds,
/* Catch all for content owned pages to differentiate from basic gallery pages */
contentElements = document.querySelectorAll('#bs-tab-links, #sliderTarget, #gallery-tab-links, #sdgSlider-4thway, #gallery-banner-tabs, .gallery-links'); 
if (data.match(/Search_Sort/)) {
valForSoasta = 'GALLERY_SEARCH';
} else if (data.match(/Search_Redirect/)) {
valForSoasta = 'GALLERY_SEARCH_REDIRECT';
} else {
valForSoasta = 'GALLERY';
}
if (contentElements !== undefined && contentElements.length !== 0) {
valForSoasta += '_CONTENT';
}
/* Hacks to identify specific content pages, needs to be removed after experiments finished */
if(window.location.href.indexOf('/women/plus-size/e/b/1589,22219.end') > -1) {
valForSoasta += '_PLUSSIZE';
}
if(window.location.href.indexOf('/home-garden/living-room/e/b/12981,21828.end') > -1) {
valForSoasta += '_LIVINGROOM';
}
if(window.location.href.indexOf('/home-garden/bedroom/e/b/12981,21822.end') > -1) {
valForSoasta += '_BEDROOM';
} 
if ( sdgGA && 
sdgGA.gallery && 
sdgGA.gallery.paginationPerPage ) {
numProds = parseInt(sdgGA.gallery.paginationPerPage, 10);
if (numProds >= 99) {
valForSoasta += '_99';
}
}
console.log(valForSoasta);
sendData(valForSoasta);
};
function pageGroupsData(section, data) {
/* Differentiate between normal, search, 12 and 99 products */
var pageIdValue = section + '_' + data.split(' | ')[3].toUpperCase();
sendData(pageIdValue);
};
function sendData(pageVarName) {
/* Set the data for collection */
SOASTA.pg = pageVarName;
};
function init(pageVarName) {
/* Execute the function */
var pageId = speedtrapId || null;
if (pageId === null) {
speedtrapCheck(100);
} else {
stringSearch(pageId);
}
};
init();
}(jQuery, window.SDCSAPageID));
</script>
